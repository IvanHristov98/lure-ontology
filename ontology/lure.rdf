    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:ID="hasEye">
        <rdfs:domain rdf:resource="#AtomicHook" />
        <rdfs:range rdf:resource="&xsd;boolean" />
    </owl:DatatypeProperty>

    <owl:Class rdf:ID="JigHead">
