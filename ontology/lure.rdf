        <rdfs:range rdf:resource="#JigHead" />
    </owl:ObjectProperty>

    <owl:DatatypeProperty rdf:ID="hasJigWeight">
        <rdfs:domain rdf:resource="#JigHead" />
        <rdfs:range rdf:resource="&xsd;float" />
    </owl:DatatypeProperty>

